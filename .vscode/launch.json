{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Command Version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/",
      "args": ["version"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Help",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["help"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Code Help",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["code", "--help"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Crawl",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["code", "TEST-001"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    }
  ]
}
