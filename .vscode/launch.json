{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Command Version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceRoot}/",
      "args": ["version"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Help",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["help"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Code Help",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["code", "--help"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Code",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["code", "--proxy=http://127.0.0.1:7890", "TEST-001"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Subtitle",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": [
        "video_subtitle",
        "--source_root_path=/mnt/h/queue/",
        "--exec=false"
      ],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Command Convert",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "showLog": true,
      "debugAdapter": "dlv-dap",
      "program": "${workspaceRoot}/",
      "args": ["convert", "--source_root_path=/mnt/h/queue/1", "--exec=false"],
      "output": "${workspaceRoot}/build/garage",
      "trace": "verbose",
      "cwd": "${workspaceRoot}"
    }
  ]
}
